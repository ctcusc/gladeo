{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start && echo \"\n\nWelcome to Gladeo! \n\nStarting Mobile app....\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest __tests__/server/ --collect-coverage ",
    "testDebug": "jest -o --watch --coverage",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "eslint --ext js,tsx {src,server}/*",
    "server": "nodemon server/server.js",
    "dev": "run-p server start",
    "clean": "rm -rf node_modules .yarnclean && yarn"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ]
  },
  "lint-staged": {
    "*.{js,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@expo/samples": "~3.0.3",
    "@expo/vector-icons": "^10.0.0",
