{
  "main": "node_modules/expo/AppEntry.js",
  "lint-staged": {
    "*.{tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "scripts": {
    "start": "expo start && echo \"\n\nWelcome to Gladeo! \n\nStarting Mobile app....\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest __tests__/server/ --detectOpenHandles",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "eslint src/**/**/**/*.tsx",
    "lint-fix": "eslint --fix src/**/**/**/*.tsx",
    "precommit": "lint-staged && yarn lint",
    "eslint-check": "eslint --print-config src/**/*/*.ts | eslint-config-prettier-check",
    "server": "nodemon server/server.js",
    "dev": "run-p server start"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ]
  },
  "dependencies": {
    "@expo/samples": "~3.0.3",
    "@expo/vector-icons": "^10.0.3",
    "@react-navigation/core": "^3.0.0-alpha.13",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "@types/jest": "^24.0.19",
    "@types/node": "^13.1.0",
    "@types/react": "^16.9.9",
    "@types/react-native": "^0.60.21",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^16.9.1",
    "airtable": "^0.8.1",
    "core-js": "^3",
    "dotenv": "^8.2.0",
    "expo": "^36.0.2",
    "expo-asset": "^8.0.0",
    "expo-constants": "^8.0.0",
    "expo-font": "^8.0.0",
    "expo-web-browser": "^8.0.0",
    "express-session": "^1.17.0",
    "mem": "^6.0.1",
    "react": "^16.12.0",
    "react-dom": "16.12.0",
    "react-native": "^0.62.0-rc.0",
    "react-native-animated-ellipsis": "^2.0.0",
    "react-native-gesture-handler": "~1.5.2",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-video": "^5.0.2",
    "react-native-web": "^0.12.0-rc.1",
    "react-navigation": "4.0.10",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-preset-expo": "^8.0.0",
    "babel-preset-react-native": "^4.0.1",
    "body-parser": "^1.19.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "express": "^4.17.1",
    "jest-expo": "^36.0.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.12.0",
    "supertest": "^4.0.2"
  },
  "private": true,
  "proxy": "http://localhost:3001"
}
